{
	"name": "@ctx-core/nav-ui-svelte",
	"version": "1.3.27",
	"description": "ctx-core nav",
	"keywords": [
		"ctx-core",
		"nav",
		"yarn"
	],
	"homepage": "https://github.com/ctx-core/nav-ui-svelte#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/nav-ui-svelte/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/nav-ui-svelte.git"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"files": [
		"*.d.ts",
		"*.js",
		"*.json",
		"*.sass",
		"./Nav_Content",
		"./Nav_Handle",
		"./Nav_Header",
		"./Nav_Item"
	],
	"types": "./src/index.d.ts",
	"exports": {
		".": "./src/index.js",
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": ":",
		"clean": ":",
		"exec": "$@",
		"prepublishOnly": "pnpm clean && pnpm build && pnpm test",
		"test": "pnpm test-unit && check-dts",
		"test-unit": "tsx node_modules/uvu/bin.js . '\\.test\\.(ts|js)$'",
		"test-unit-coverage": "c8 pnpm test-unit"
	},
	"dependencies": {
		"@ctx-core/fontawesome-ui-svelte": "1.1.26",
		"@ctx-core/nav": "^23.1.25",
		"@ctx-core/svelte": "^15.0.7",
		"@ctx-core/object": "^31.1.4",
		"svelte": "^4.2.7"
	},
	"devDependencies": {
		"c8": "^8.0.1",
		"check-dts": "^0.7.2",
		"tsx": "^4.2.0",
		"typescript": "next",
		"uvu": "^0.5.6"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	},
	"sideEffects": false
}